{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "outDir": "../out-tsc/app",
    "module": "commonjs",
    "target": "es5",
    "types": ["jasmine", "jasminewd2", "node"],
    "baseUrl": "./",
    "paths": {
      "@env": ["../src/environments/environment"],
      "@shared/*": ["../src/app/shared/*"],
      "@core/*": ["../src/app/core/*"],
      "@store": ["../src/app/root-store"],
      "@store/*": ["../src/app/root-store/*"]
    }
  }
}
